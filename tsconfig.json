{
    "compilerOptions": {
        "moduleResolution": "node",
        "target": "ES5",
        "lib": [
            "ESNext"
        ],
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "noEmit": true
    }
}
